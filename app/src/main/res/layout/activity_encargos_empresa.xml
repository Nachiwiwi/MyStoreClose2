<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#222831"
    tools:context=".EncargosEmpresa">

    <ListView
        android:id="@+id/listaCompradores"
        android:layout_width="342dp"
        android:layout_height="463dp"
        android:layout_marginBottom="61dp"
        android:background="@color/colorBoton2"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/botonVolverPantalla12" />

    <ImageButton
        android:id="@+id/botonVolverPantalla12"
        android:layout_width="97dp"
        android:layout_height="86dp"
        android:layout_marginStart="39dp"
        android:layout_marginTop="22dp"
        android:layout_marginBottom="90dp"
        android:contentDescription="Volver"
        app:layout_constraintBottom_toTopOf="@+id/listaCompradores"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="?attr/actionModeCloseDrawable" />
</androidx.constraintlayout.widget.ConstraintLayout>